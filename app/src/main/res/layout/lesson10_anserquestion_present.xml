<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".lesson10AnserquestionPresent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView1304"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1.	What does terry want?"
                android:textSize="18sp" />

            <Spinner
                android:id="@+id/QspsL101"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/textView1305"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2.	Does he have money?"
                android:textSize="18sp" />

            <Spinner
                android:id="@+id/QspsL102"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/textView1306"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="3.	What color does he want it?"
                android:textSize="18sp" />

            <Spinner
                android:id="@+id/QspsL103"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/textView1307"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="4.	Where does Terry live?"
                android:textSize="18sp" />

            <Spinner
                android:id="@+id/QspsL104"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/textView1308"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="5.	Does he have the cash?"
                android:textSize="18sp" />

            <Spinner
                android:id="@+id/QspsL105"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/textView1309"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="6.	Where does he go?"
                android:textSize="18sp" />

            <Spinner
                android:id="@+id/QspsL106"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/textView1310"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="7.	What does he do?"
                android:textSize="18sp" />

            <Spinner
                android:id="@+id/QspsL107"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <TextView
                android:id="@+id/textView1311"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="8.	How many flies does he catch?"
                android:textSize="18sp" />

            <Spinner
                android:id="@+id/QspsL108"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/textView1312"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="9.	Who does he sell them to?"
                android:textSize="18sp" />

            <Spinner
                android:id="@+id/QspsL109"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/textView1313"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10.	Does he buy a brand new, bright green, super-fast, Ferrari?"
                android:textSize="18sp" />

            <Spinner
                android:id="@+id/QspsL1010"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <Button
                android:id="@+id/button103"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="#E7323D"
                android:onClick="qualification"
                android:text="Check Score"
                android:textColor="#FFFFFF"
                android:layout_gravity="center_horizontal|center_vertical"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>