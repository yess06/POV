<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".lesson9AnswerVocabulary">

    <TextView
        android:id="@+id/textView1112"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="As a rule : Generally; usually"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView1170"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Everything but the kitchen sink : Almost everything possible"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView1171"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Play a major role : Is a major influence or reason"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView1172"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Preachin' to the choir : Trying to persuade someone who already agrees"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView1173"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Seen the light : Finally understand"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView1174"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="To go in one ear and out the other : To be ignored or disregared"
        android:textSize="18sp" />

    <Button
        android:id="@+id/button97"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:background="#207A7A"
        android:onClick="practicepage"
        android:text="More Practice"
        android:textColor="#FFFFFF" />

</LinearLayout>