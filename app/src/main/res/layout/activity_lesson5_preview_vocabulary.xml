<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".lesson5_preview_vocabulary">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView1319"
            android:layout_width="93dp"
            android:layout_height="wrap_content"
            android:text="Can't stand"
            android:textSize="18sp" />

        <Spinner
            android:id="@+id/qspsL22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp" />

        <TextView
            android:id="@+id/textView1324"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1. A lot"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView1320"
            android:layout_width="93dp"
            android:layout_height="wrap_content"
            android:text="Out of hand"
            android:textSize="18sp" />

        <Spinner
            android:id="@+id/qspsL23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp" />

        <TextView
            android:id="@+id/textView1325"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2. Do not like"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView1321"
            android:layout_width="93dp"
            android:layout_height="wrap_content"
            android:text="Quite a few"
            android:textSize="18sp" />

        <Spinner
            android:id="@+id/qspsL24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp" />

        <TextView
            android:id="@+id/textView1326"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3. Very bad"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView1322"
            android:layout_width="105dp"
            android:layout_height="wrap_content"
            android:text="In over his head"
            android:textSize="18sp" />

        <Spinner
            android:id="@+id/qspsL25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp" />

        <TextView
            android:id="@+id/textView1327"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4. To not happen"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView1323"
            android:layout_width="84dp"
            android:layout_height="wrap_content"
            android:text="Fell through"
            android:textSize="18sp" />

        <Spinner
            android:id="@+id/qspsL26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp" />

        <TextView
            android:id="@+id/textView1328"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5. To be involved with something that is too difficult to do"
            android:textSize="18sp" />
    </LinearLayout>

    <Button
        android:id="@+id/btnlessontwopreviewvocabulary8"
        android:layout_width="106dp"
        android:layout_height="48dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:background="#E7323D"
        android:onClick="postqualification"
        android:text="Ready"
        android:textColor="#FFFFFF" />

</LinearLayout>