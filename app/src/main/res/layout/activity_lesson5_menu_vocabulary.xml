<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".lesson5_menu_vocabulary">
    <Button
        android:id="@+id/btnlessontwoPreview"
        android:layout_width="337dp"
        android:layout_height="122dp"
        android:layout_marginTop="216dp"
        android:background="#EBEBEB"
        android:drawableLeft="@drawable/preview"
        android:text="Preview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnlessontwoPractice"
        android:layout_width="337dp"
        android:layout_height="122dp"
        android:background="#EBEBEB"
        android:drawableLeft="@drawable/practice"
        android:text="Practice"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnlessontwoPreview"
        app:layout_constraintVertical_bias="0.173" />
</androidx.constraintlayout.widget.ConstraintLayout>