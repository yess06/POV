<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".lesson5_answer_vocabulary">

    <TextView
        android:id="@+id/textView793"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Blown away : Pleased and very surprised"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView824"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Can't stand : Do not like"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView825"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Could not believe his eyes : What you see is not believable"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView826"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Fell through : To not happen"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView827"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="In over his head : To be inolved with something that is too difficult to do"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView828"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Out of hand : Very bad"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView829"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Quite a few : A lot"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView830"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="To not know what you're getting into : To not know what to expect"
        android:textSize="18sp" />

    <Button
        android:id="@+id/button80"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="112dp"
        android:background="#207A7A"
        android:onClick="practicepage"
        android:text="More Practice"
        android:textColor="#FFFFFF" />

</LinearLayout>