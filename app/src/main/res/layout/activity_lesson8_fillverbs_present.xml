<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".lesson8_fillverbs_present">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView943"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Leo the lion"
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL81"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:id="@+id/textView944"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="excited.  "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView945"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="He finally"
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL82"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/textView946"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="enough money to buy a ticket to fly to the moon.   "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView947"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="One week before leaving he"
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL83"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView948"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="to a bar with his friend Samuel the snake."
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView949"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Samuel and Leo"
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL84"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/textView950"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="twelve beers each.  "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView951"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Samuel"
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL85"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:id="@+id/textView952"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="so drunk that he falls on the floor.   "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView953"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="The bartender then"
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL86"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView954"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="“I had no idea you were so drunk.  It is time for you to go home.”   "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView955"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="So Leo and Samuel"
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL87"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/textView956"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="the bar.  "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView957"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Samuel "
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL88"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/textView958"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="to drive his car home.   "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView959"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" “If you drive now, you will have a run-in with the law,” Leo"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <Spinner
                    android:id="@+id/fillPsL89"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView961"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" But Samuel "
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL810"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/textView962"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="his car anyway.   "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView963"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" After driving one mile, he is pulled over. "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView964"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="The policeman "
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL811"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:id="@+id/textView965"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="that Samuel is drunk.   "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView966"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="“Now you’re going to be thrown in jail,” "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView967"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="the policeman.   "
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL812"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView968"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" The next day Samuel "
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL813"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView969"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Leo to borrow money so he can hire a lawyer.   "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView970"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" “I hope that he can get me off.  I don’t want them to throw the book at me,” Samuel."
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <Spinner
                    android:id="@+id/fillPsL814"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView972"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" Leo"
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL815"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/textView973"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Samuel money for the lawyer.   "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView974"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" “Now I have no money.  I can’t go to the moon,” says Leo.   "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView975"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" He is very upset.  "
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView976"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="He"
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/fillPsL816"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/textView977"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="he should not have let Samuel drive. "
                    android:textSize="18sp" />
            </LinearLayout>

            <Button
                android:id="@+id/button87"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|center_vertical"
                android:layout_marginTop="10dp"
                android:background="#E7323D"
                android:text="Check Score"
                android:textColor="#FFFFFF" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>